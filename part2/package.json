{
  "name": "about-testing",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^1.4.0",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^9.5.0",
    "@types/enzyme": "^3.10.8",
    "@types/node": "^12.12.53",
    "@types/react-dom": "^16.9.8",
    "bootstrap": "^4.5.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "node-sass": "^4.14.1",
    "react": "^16.13.1",
    "react-bootstrap": "^1.4.0",
    "react-dom": "^16.13.1",
    "react-icons": "^3.10.0",
    "react-scripts": "^3.4.3",
    "react-tooltip": "^4.2.10",
    "react-transition-group": "^4.4.1",
    "typescript": "^3.8.3"
  },
  "scripts": {
    "start": "GENERATE_SOURCEMAP=true react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "coverage": "npm run test -- --watch --coverage --coverage-reporters=lcov --watchAll=false",
    "coverageWatch": "npm run test -- --watch --coverage --coverage-reporters=lcov --watchAll=true"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/jest": "^24.9.1",
    "@types/react": "^16.9.49"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,ts,tsx}",
      "!src/serviceWorker.{ts,js}",
      "!src/index.ts",
      "!src/unit-tests/**/*",
      "!src/sagas/**",
      "!src/unit-tests/sagas/sagas.test.js"
    ]
  }
}
